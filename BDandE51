#include <stdio.h>

void BDandE51() {
    int array[5];
    for (int i = 0; i < 5; i++) {
        printf("Enter the value of index %d: ", i);
        scanf("%d", &array[i]);
    }
    int k = 0;
    for (int j = 4; j >= 0; j--) {
        printf("array_n[%d] = %d\n", k, array[j]);
        k++;
    }
}

int main(void) {
    BDandE51();
    return 0;
}
