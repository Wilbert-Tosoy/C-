#include <stdio.h>

void BDandE40() {
    int x, y;
    printf("Enter the first number: ");
    scanf("%d", &x);
    printf("Enter the second number: ");
    scanf("%d", &y);
    for (int i = x; i < y; i++) {
        if (i % 7 == 2 || i % 7 == 3) {
            printf("%d ", i);
        } 
    }
}

int main(void) {
    BDandE40();
    return 0;
}
