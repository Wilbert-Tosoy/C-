#include <stdio.h>

void swappingValues() {
    int x, y;
    printf("Enter the value of x: ");
    scanf("%d", &x);
    printf("Enter the value of y: ");
    scanf("%d", &y);
    printf("Before the swapping the value of x & y: %d %d\n", x, y);
    x = x + y;
    y = x - y;
    x = x - y;
    printf("After swapping the values of x & y: %d %d", x, y);
}

int main(void) {
    swappingValues();
    return 0;
}
