#include <stdio.h>

void simplePassword() {
    int password = 1234;
    int x;
    do {
        printf("Enter password: ");
        scanf("%d", &x);
        if (x == password) {
            printf("Correct Password");
        } else {
            printf("Incorrect Password\n");
        }
    } while (x != password);
}

int main(void) {
    simplePassword();
    return 0;
}
