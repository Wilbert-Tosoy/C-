#include <stdio.h>

void BDandE37() {
    int x, y;
    printf("Enter the x: ");
    scanf("%d", &x);
    printf("Enter the y: ");
    scanf("%d", &y);
    if (x > 0 && y > 0) {
        printf("Quadrant-1 (+, +)");
    } else if (x < 0 && y < 0) {
        printf("Quadrant-3 (-, -)");
    } else if (x < 0 && y > 0) {
        printf("Quadrant-2 (-, +)");
    } else {
        printf("Quadrant-4 (+, -)");
    }
}

int main(void) {
    BDandE37();
    return 0;
}
