#include <stdio.h>

void BDandE34() {
    int x, y;
    int sum = 0;
    printf("Enter the first number of the pair: ");
    scanf("%d", &x);
    printf("Enter the second number of the pair: ");
    scanf("%d", &y);
    for (int i = y; i <= x; i++) {
        if (i % 2 != 0) {
            printf("%d\n", i);
            sum += i;
        }
    }
    printf("Sum: %d", sum);
}

int main(void) {
    BDandE34();
    return 0;
}
