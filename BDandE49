#include <stdio.h>

void BDandE49() {
    int array[5];
    int x;
    printf("Enter the first number: ");
    scanf("%d", &x);
    for (int i = 0; i < 5; i++) {
        array[i] = x;
        x = x * 3;
    }
    for (int i = 0; i < 5; i++) {
        printf("n[%d] = %d\n", i, array[i]);
    }
    
}

int main(void) {
    BDandE49();
    return 0;
}
