#include <stdio.h>

void BDandE50() {
    int array[5];
    for (int i = 0; i < 5; i++) {
        printf("Enter the value of index number %d: ", i);
        scanf("%d", &array[i]);
    }
    for (int i = 0; i < 5; i++) {
        if (array[i] < 5) {
            printf("A[%d] = %d\n", i, array[i]);
        }
    }
}

int main(void) {
    BDandE50();
    return 0;
}
