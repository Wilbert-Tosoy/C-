#include <stdio.h>
#include <math.h>

void squareAndCubeInALine() {
    int x;
    printf("Enter a number: ");
    scanf("%d", &x);
    for (int i = 1; i <= x; i++) {
        double squareRoot = pow(i, 2);
        double cubeRoot = pow(i, 3);
        printf("%d %.0lf %.0lf\n", i, ceil(squareRoot), ceil(cubeRoot));
    }
}

int main(void) {
    squareAndCubeInALine();
    return 0;
}
